<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="AccelerateFeed" value="https://accelerate-nuget-feed.avaloniaui.net/v3/index.json" />
    <add key="InternalFeed" value="https://internal-nuget-feed.avaloniaui.net/v3/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <AccelerateFeed>
      <add key="Username" value="license" />
      <add key="ClearTextPassword" value="%AccelerateLicenseKey%" />
    </AccelerateFeed>
    <InternalFeed>
      <add key="Username" value="general" />
      <add key="ClearTextPassword" value="%InternalNuGetKey%" />
    </InternalFeed>
  </packageSourceCredentials>
</configuration>
